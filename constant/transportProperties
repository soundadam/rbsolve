FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

// 运动粘度
nu              [0 2 -1 0 0 0 0] 1.5e-5;

// 热膨胀系数
beta            [0 0 0 -1 0 0 0] 0.00333;

// 分子普朗特数 (Molecular Prandtl number)
Pr              [0 0 0 0 0 0 0] 0.71;

// 湍流普朗特数 (Turbulent Prandtl number) - 填入标准值 0.85
Prt             [0 0 0 0 0 0 0] 0.85;

// 参考温度
TRef            [0 0 0 1 0 0 0] 300;

